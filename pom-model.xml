      <groupId>it.unimi.dsi</groupId>
      <artifactId>sux4j</artifactId>
      <version>5.2.3</version>
    </dependency>
    <dependency>
      <groupId>it.unimi.di</groupId>
      <artifactId>jsap</artifactId>
      <version>20210129</version>
    </dependency>

